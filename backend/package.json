{
    "name": "ecotrack-backend",
    "version": "1.0.1",
    "description": "Backend for EcoTrack app",
    "main": "server.js",
    "scripts": {
        "release": "standard-version",
        "test": "echo \"Run all tests\" && exit 0",
        "test:unit": "jest --coverage --outputFile=backend/reports/unit/test-results.xml",
        "test:integration": "mocha --reporter mocha-junit-reporter --reporter-options mochaFile=backend/reports/integration/test-results.xml",
        "test:e2e": "cypress run --reporter junit --reporter-options mochaFile=backend/reports/e2e/test-results.xml"
    },
    "dependencies": {
        "@google/generative-ai": "^0.24.0",
        "axios": "^1.6.2",
        "bcryptjs": "^2.4.3",
        "cors": "^2.8.5",
        "dotenv": "^16.3.1",
        "express": "^4.18.2",
        "express-validator": "^7.2.1",
        "jsonwebtoken": "^9.0.2",
        "mongoose": "^8.0.3",
        "morgan": "^1.10.0",
        "openai": "^4.28.0"
    },
    "devDependencies": {
        "jest": "^29.7.0",
        "jest-junit": "^16.0.0",
        "nodemon": "^3.0.2",
        "standard-version": "^9.5.0",
        "supertest": "^7.1.0"
    }
}
